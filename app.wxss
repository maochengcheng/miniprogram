/*底部按钮*/

.bottom-btn {
  height: 50px;
  background-color: #2987f9;
  opacity: 1;
  color: white;
  line-height: 50px;
  width: 100% !important;
  text-align: center;
  font-size: 40rpx;
  margin: 0px !important;
  padding: 0px;
  border-radius: 0px;
  font-weight: normal;
}

.bottom-btn-two {
  height: 45px;
  border: 1px dashed rgba(38, 153, 251, 1);
  opacity: 1;
  line-height: 45px;
  text-align: center;
  border-radius: 2rem;
  color: #00a0e9;
  background-color: white;
  font-size: 40rpx;
}


