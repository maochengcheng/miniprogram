/** position **/

.posi-rela {
  position: relative;
}

/*铺满*/
.posi-fixed{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}

.posi-all {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

/*居中*/

.posi-center {
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  margin: 0 auto;
  transform: translateY(-50%);
}

.posi-right-top{
  position: absolute;
  right: -10rpx;
  top: -10rpx;

}